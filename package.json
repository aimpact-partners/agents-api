{
	"name": "@aimpact/agents-api",
	"version": "0.4.1",
	"title": "Agents API for custom chats using Agents",
	"description": "",
	"license": "ISC",
	"scripts": {
		"start": "node index.js"
	},
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"params": {
		"local": {
			"wsUrl": "ws://localhost:5040"
		},
		"development": {
			"wsUrl": "wss://dev.agents.api.aimpact.partners"
		},
		"testing": {
			"wsUrl": "wss://test.agents.api.aimpact.partners"
		},
		"quality": {
			"wsUrl": "wss://beta.agents.api.aimpact.partners"
		},
		"production": {
			"wsUrl": "wss://agents.api.aimpact.partners"
		}
	},
	"deployment": {
		"distributions": [
			{
				"name": "http",
				"environment": "local",
				"platform": "node",
				"ports": {
					"bundles": 6583,
					"http": 6584,
					"inspect": 6585
				}
			},
			{
				"name": "http-development",
				"environment": "development",
				"platform": "node"
			},
			{
				"name": "http-testing",
				"environment": "testing",
				"platform": "node"
			},
			{
				"name": "http-beta",
				"environment": "quality",
				"platform": "node"
			},
			{
				"name": "http-production",
				"environment": "production",
				"platform": "node"
			},
			{
				"name": "local-web",
				"environment": "local",
				"platform": "web",
				"ports": {
					"bundles": 6400
				}
			},
			{
				"name": "local-web",
				"environment": "local",
				"platform": "web",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 6401
				}
			},
			{
				"name": "node-tsc",
				"environment": "local",
				"platform": "node",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 6551
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/firestore-collection": "~0.0.9",
		"@beyond-js/events": "~0.0.7",
		"@beyond-js/response": "~0.0.3",
		"@google-cloud/storage": "^7.7.0",
		"express": "~4.21.1",
		"express-openapi-validator": "~5.3.9",
		"firebase-admin": "^12.7.0",
		"multer": "^1.4.5-lts.1",
		"form-data": "^4.0.0",
		"jsonwebtoken": "^9.0.1",
		"ws": "~8.18.0",
		"socket.io": "~4.8.1",
		"node-fetch": "^2.6.12",
		"dotenv": "^16.0.3",
		"fluent-ffmpeg": "^2.1.2",
		"dayjs": "^1.11.7",
		"openai": "~4.83.0",
		"uuid": "^10.0.0",
		"find-up": "~7.0.0"
	},
	"devDependencies": {
		"socket.io-client": "^4.5.4",
		"@beyond-js/react-18-widgets": "~1.1.2",
		"@beyond-js/bee": "0.0.6",
		"@beyond-js/local": "~0.1.3",
		"@types/busboy": "1.5.3",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/express": "~5.0.0",
		"@types/node": "~20.6.0",
		"@types/uuid": "~9.0.7",
		"@types/ws": "~8.5.14",
		"@types/react": "~18.3.11",
		"@types/audioworklet": "~0.0.71",
		"swagger-ui-express": "^5.0.0",
		"yaml": "^2.3.1"
	}
}
